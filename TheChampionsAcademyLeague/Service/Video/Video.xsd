<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/TheChampionsAcademyLeague/Schemas/Schema.xsd"
	 targetNamespace="http://www.tibco.com/schemas/TheChampionsAcademyLeague/Schemas/Schema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="videoInput">
		<xs:complexType>
			<xs:attribute name="ID_Video" type="xs:string"/>
			<xs:attribute name="VideoName" type="xs:string"/>
			<xs:attribute name="Duration" type="xs:string"/>
			<xs:attribute name="Topic" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="videoOutput">
		<xs:complexType>
			<xs:attribute name="ID_Video" type="xs:string"/>
			<xs:attribute name="VideoName" type="xs:string"/>
			<xs:attribute name="Duration" type="xs:string"/>
			<xs:attribute name="Topic" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="VideoDataInput">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="videoInput" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="VideoDataOutput">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="videoOutput" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>